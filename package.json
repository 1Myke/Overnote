{
    "bin": {"overnote": "jdeploy-bundle/jdeploy.js"},
    "author": "Stack Underflow",
    "description": "Note taking app powered by AI",
    "main": "index.js",
    "preferGlobal": true,
    "repository": {"type": "git", "url": "https://github.com/1Myke/Overnote"},
    "version": "1.0.0",
    "jdeploy": {
        "jdk": false,
        "publishTargets": [{
